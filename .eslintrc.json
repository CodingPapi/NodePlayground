{
    "extends": [
        "airbnb",
        "plugin:flowtype/recommended"
    ],
    "plugins": [
        "flowtype"
    ],
    "rules": {
        "semi": [2, "never"],
        "no-unexpected-multiline": 2,
        "indent": ["error", 4]
    },
    "env": {
        "jest": true,
        "es6": true
    }

}
